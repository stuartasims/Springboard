   "source": [
    "#Assemble the data as total sentences and number of executions\n",
    "\n",
    "state_counts = pd.pivot_table(df[['executed', 'state']],\n",
    "                              index='state',\n",
    "                              columns='executed',\n",
    "                              aggfunc=len,\n",
    "                              fill_value=0)\n",
    "state_counts['total'] = state_counts['N'] + state_counts['Y']\n",
    "state_counts = state_counts.drop(labels='N', axis=1)\n",
    "\n",
    "#create fields to size each point for aesthetics in plot\n",
    "scaler = 3.5\n",
    "state_counts['size_e'] = state_counts['Y'] * scaler\n",
    "state_counts['size_t'] = state_counts['total'] * scaler\n",
    "\n",
    "#plot the number of executions and total sentences as a categorical bubble plot\n",
    "plt.figure(figsize=(16, 10))\n",
    "scatterplot = plt.scatter(x=state_counts.index,\n",
    "                          y='total',\n",
    "                          s='size_t',\n",
    "                          alpha=0.5,\n",
    "                          color='blue',\n",
    "                          data=state_counts)\n",
    "plt.scatter(x=state_counts.index,\n",
    "            y='total',\n",
    "            s='size_e',\n",
    "            alpha=0.7,\n",
    "            color='red',\n",
    "            data=state_counts)\n",
    "\n",
    "#add legend in middle right\n",
    "plt.legend(\n",
    "    ['sentences (sen)', 'executions (ex)'],\n",
    "    loc='center right',\n",
    "    markerscale=0.75,\n",
    "    labelspacing=2,\n",
    "    fancybox=True,\n",
    "    frameon=True,\n",
    "    borderpad=2,\n",
    ")\n",
    "\n",
    "#remove axes ticks\n",
    "plt.yticks([])\n",
    "plt.xticks([])\n",
    "plt.title(\n",
    "    'Sentences to Death Row and Executions by State (or prosecuting body)',\n",
    "    fontsize=16)\n",
    "\n",
    "#label each point with the state abbreviation, sentences, and executions; offset the labels below points\n",
    "for i, j in enumerate(state_counts['total']):\n",
    "    plt.annotate(\n",
    "        'state: ' + state_counts.index[i] + '\\nsen: ' + str(j) + '\\n ex: ' +\n",
    "        str(state_counts['Y'][i]),\n",
    "        (state_counts.index[i],\n",
    "         state_counts['total'][i] - 0.13 * state_counts['total'][i]))"
   ]
